<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.k2infosoft.pinterestlogin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="pdk[4871278455632708780]" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_kitchen_sink" />
        <activity
            android:name=".MyPinsActivity"
            android:label="@string/title_activity_my_pins" />
        <activity
            android:name=".CreatePinActivity"
            android:label="@string/title_activity_create_board" />
        <activity
            android:name=".MyBoardsActivity"
            android:label="@string/title_activity_my_boards" />
        <activity
            android:name=".CreateBoardActivity"
            android:label="@string/title_activity_create_pin" />
        <activity
            android:name=".AnyPathActivity"
            android:label="@string/title_activity_any_path" />
        <activity
            android:name=".FollowingActivity"
            android:label="@string/title_activity_following" />
        <activity
            android:name=".SearchUserActivity"
            android:label="@string/title_activity_search_user" />
    </application>

</manifest>